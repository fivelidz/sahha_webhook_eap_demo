[Unit]
Description=Sahha EAP Dashboard
After=network.target

[Service]
Type=simple
User=fivelidz
WorkingDirectory=/home/fivelidz/sahha_work/sahha-marketing-intelligence/projects/sahha-wellness-template/react-version/profile-manager
ExecStart=/usr/bin/npm run dev
Restart=always
RestartSec=10
StandardOutput=append:/home/fivelidz/sahha_work/sahha-marketing-intelligence/projects/sahha-wellness-template/react-version/profile-manager/server.log
StandardError=append:/home/fivelidz/sahha_work/sahha-marketing-intelligence/projects/sahha-wellness-template/react-version/profile-manager/server.log
Environment=NODE_ENV=development
Environment=PORT=3000

[Install]
WantedBy=multi-user.target